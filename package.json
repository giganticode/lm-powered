{
	"name": "visualizationtool",
	"displayName": "IDE Visualization",
	"description": "",
	"version": "0.0.1",
	"publisher": "unibz",
	"repository": {
		"type": "git",
		"url": "git://github.com/giganticode/vsc-extension-backend.git"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "IDE Visualizations",
			"type": "object",
			"properties": {
				"visualization.general.colorranges": {
					"type": "array",
					"default": [
						{
							"Minimum": 0,
							"Maximum": 2,
							"Color": "green"
						},
						{
							"Minimum": 2,
							"Maximum": 4,
							"Color": "rgb(126, 128, 0)"
						},
						{
							"Minimum": 4,
							"Maximum": 8,
							"Color": "orange"
						},
						{
							"Minimum": 8,
							"Maximum": 16,
							"Color": "rgb(255, 72, 0)"
						},
						{
							"Minimum": 16,
							"Maximum": 25,
							"Color": "red"
						}
					],
					"description": "Indicates the upper bound bound of the ranges"
				},
				"visualization.exclude.directories": {
					"type": "array",
					"default": [
						"node_modules",
						"out"
					],
					"description": "Determines the folder names that sould not be considered for search and treemap"
				},
				"visualization.exclude.filetype": {
					"type": "array",
					"default": [
						".png",
						".jpg",
						".jpeg",
						".svg"
					],
					"description": "Determines the file types that sould not be considered for search and treemap"
				},
				"visualization.risk.enabled": {
					"type": "boolean",
					"default": true,
					"description": "Risk enabled"
				},
				"visualization.autocompletion.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Autocompletion enabled"
				},
				"visualization.autocompletion.url": {
					"type": "string",
					"default": "http://localhost:8080/api/autocompletion",
					"description": "Determines the hostname of the webservice for the language model autocompletion"
				},
				"visualization.folding.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Folding enabled"
				},
				"visualization.folding.foldonopen": {
					"type": "boolean",
					"default": false,
					"description": "Fold low risk level code when opening"
				},
				"visualization.folding.maxrisk": {
					"type": "number",
					"default": 35,
					"description": "Max risk level for folding"
				},
				"visualization.languagemodel.model": {
					"type": "string",
					"default": "langmodel-large-split_10k_2_1024_191007.112241_-_langmodel-large-split_10k_2_1024_191022.141344",
					"description": "Indicates the name of the languagemodel to use for entropy calculation",
					"enum": [
						"langmodel-large-split_10k_2_1024_191007.112241_-_langmodel-large-split_10k_2_1024_191022.141344",
						"langmodel-large-split_10k_3_1024_191007.112257_-_langmodel-large-split_10k_3_1024_191022.134822",
						"langmodel-large-split_10k_2_2048_191007.112249_-_langmodel-large-split_10k_2_2048_191022.141335",
						"langmodel-large-split_10k_1_512_190926.120146",
						"langmodel-small-split_10k_1_512_190906.154943",
						"dev_10k_1_10_190923.132328"
					],
					"enumDescriptions": [
						"Default/Best - langmodel-large-split_10k_2_1024_191007.112241_-_langmodel-large-split_10k_2_1024_191022.141344",
						"langmodel-large-split_10k_3_1024_191007.112257_-_langmodel-large-split_10k_3_1024_191022.134822",
						"langmodel-large-split_10k_2_2048_191007.112249_-_langmodel-large-split_10k_2_2048_191022.141335",
						"Medium - langmodel-large-split_10k_1_512_190926.120146",
						"Tiny - langmodel-small-split_10k_1_512_190906.154943",
						"Random - dev_10k_1_10_190923.132328"
					]
				},
				"visualization.debugmode.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enable debug mode - visualize token entropies"
				},
				"visualization.languagemodel.url": {
					"type": "string",
					"default": "http://localhost:8080/api/languagemodel",
					"description": "Determines the hostname of the webservice for the language model"
				},
				"visualization.minimap.enabled": {
					"type": "boolean",
					"default": true,
					"description": "Minimap highlighting enabled"
				},
				"visualization.minimap.minrisk": {
					"type": "number",
					"default": 16,
					"description": "Determines the minimal risk level for which a highlighting is shown"
				},
				"visualization.search.url": {
					"type": "string",
					"default": "http://localhost:8080/api/search",
					"description": "Determines the hostname of the webservice"
				},
				"visualization.search.matchindicator": {
					"type": "number",
					"default": 20,
					"description": "When the difference in % is bigger than the specified value, the token matches to search query"
				},
				"visualization.search.queryinterval": {
					"type": "number",
					"default": 10,
					"description": "Interval: after each n-th line a search-quer-comment is inserted"
				}
			}
		},
		"commands": [
			{
				"command": "ide_visualization.showOverview",
				"title": "Treemap overview - Show a report of your code quality",
				"category": "Languagemodel"
			},
			{
				"command": "ide_visualization.showSearch",
				"title": "Contetual search - Send contextual search queries to the languagemodel",
				"category": "Languagemodel"
			},
			{
				"command": "ide_visualization.showDebug",
				"title": "Enable debug mode (visualize token entropies)",
				"category": "Languagemodel"
			},
			{
				"command": "ide_visualization.clearDebug",
				"title": "Disable debug mode (hide token entropies)",
				"category": "Languagemodel"
			}
		],
		"keybindings": [
			{
				"command": "ide_visualization.showOverview",
				"key": "alt+m",
				"mac": "cmd+o",
				"when": "editorTextFocus"
			},
			{
				"command": "ide_visualization.showSearch",
				"key": "alt+shift+s",
				"mac": "cmd+o",
				"when": "editorTextFocus"
			}
		]
	},
	"keybindings": [],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"dom-parser": "^0.1.6",
		"fs": "0.0.1-security",
		"jquery": "^3.4.1",
		"jsdom": "^15.1.1",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.26",
		"nouislider": "^13.1.5",
		"opn": "^4.0.2",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.7",
		"squarify": "^1.1.0",
		"ts-md5": "^1.2.4",
		"url-query-builder": "^3.0.0",
		"vscode-languageclient": "^5.2.1"
	},
	"workbench.experimental.colorCustomizations": {
		"editorLineNumbers": "#FF0000"
	}
}
